---
batch_connect:
  template: "vnc"
script:
  queue_name: <%= custom_queue %>
  native:
    <%- if custom_queue == 'gpu' -%>
    - '--gres'
    - 'gpu:1'
    <%- end -%>
    - "-c"
    - "<%= num_cores.blank? ? 1 : num_cores.to_i %>"
    - "-t"
    - "<%= wall_time %>"
    - "--mem"
    - "<%= memory.blank? ? 2000 : memory.to_i * 1000 %>"
